{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "prettier",
    "airbnb-base"
  ],
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    // GENERAL
    "no-throw-literal": 0,
    "operator-linebreak": 0,
    "no-use-before-define": 0,
    "no-case-declarations": 0,
    "prefer-promise-reject-errors": 0,
    "implicit-arrow-linebreak": 0,
    "no-prototype-builtins": 0,
    "object-curly-newline": 0,
    "no-useless-catch": 0,
    "global-require": 0,
    "no-extra-semi": 0,
    "no-shadow": 0,
    "quotes": 0,
    "indent": 0,
    "radix": 0,
    "new-cap": 0,
    "no-alert": 0,
    "no-console": 0,
    "func-names": 0,
    "no-plusplus": 0,
    "comma-dangle": 0,
    "no-unused-vars": 0,
    "no-param-reassign": 0,
    "consistent-return": 0,
    "class-methods-use-this": 0,
    "prettier/prettier": [
      "error",
      {
        "singleQuote": false,
        "printWidth": 100,
        "semi": true,
        "tabWidth": 4,
        "useTabs": false
      }
    ]
  }
}
